{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "@constants": ["./src/constants"],
      "@types": ["./src/types"],
      "@utils/*": ["./src/utils/*"],
      "@data/*": ["./src/data/*"],
      "@actions": ["./src/actions/"],
      "@signals/*": ["./src/signals/*"],
      "@services/*": ["./src/services/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}